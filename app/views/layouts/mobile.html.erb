<!DOCTYPE html>
<%# "Layout used only for test page" %>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="keywords" content="<%= brand_name %>">
    <meta name="author" content="<%= brand_name %>">
    <meta name="description" content="<%= content_for?(:description) ? yield(:description) : I18n.t('sales.description') %>">
    <meta property="og:type" content="website">
    <meta property="og:site_name" content="<%= I18n.t('brand.company') %>">
    <meta name="robots" content="index, follow">
    <link rel="image_src" href="<%= image_url 'images/verde-circle-logo-name-centered.png' %>">
    <%= favicon_link_tag %>
    <title>
      <%= content_for?(:title) ? yield(:title) : I18n.t('sales.title') %>
    </title>
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= stylesheet_link_tag 'verdecircle', media: 'all' %>
    <% if content_for :stylesheets %>
      <%= yield :stylesheets %>
    <% end %>
    <!--[if lt IE 9]
      <%= javascript_include_tag 'ie'%>
    -->
    <%= csrf_meta_tags %>
    <%= javascript_include_tag 'head' %>
    <%= javscript_include_tag 'application' %>
    <%= yield :head %>
  </head>
  <body class="controller__<%= controller_name %> action__<%= action_name %>" data-feed="{<%= %Q("current_user":{"id":"#{current_user.id if user_signed_in?}","username":"#{current_user.username if user_signed_in?}"}) %>}">
    <%= render 'layouts/navbars/test' %>
    <div class="container-fluid">
      <div class="errors__container">
        <%= render 'layouts/messages' %>
      </div>
        <%= yield %>
    </div> <!-- END .container-fluid -->
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>

    <%= javascript_include_tag 'global' %>
    <%= javascript_include_tag 'community_center' %>
    <%= render 'layouts/footers/verdecircle' %>
    <%= render 'layouts/javascripts' %>
    <%= render 'layouts/analytics' %>
  </body>
</html>
